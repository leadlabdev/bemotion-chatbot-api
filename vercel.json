{
  "version": 2,
  "builds": [
    {
      "src": "src/lambda.ts",
      "use": "@vercel/node",
      "env": {
        "NODE_OPTIONS": "--require tsconfig-paths/register"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/lambda.ts"
    }
  ],
  "buildCommand": "npm run build && dir dist /s"
}
